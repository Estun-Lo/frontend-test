{
    "name": "asknicely/frontend-test",
    "description": "The AskNicely frontend test",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "silex/silex": "~2.3",
        "silex/web-profiler": "~2.0",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "rpodwika/yaml-config-service-provider": "dev-master",
        "doctrine/dbal": "^3.3",
        "symfony/asset": "^3.2",
        "symfony/twig-bridge": "^3.2",
        "symfony/webpack-encore-bundle": "*",
        "symfony/assetic-bundle": "dev-master"
    },
    "require-dev": {
        "symfony/browser-kit": "^3.2"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "scripts": {
        "start": [
            "echo 'Started web server on http://localhost:1337'",
            "php -S localhost:1337 -t web/ web/index.php"
        ]
    }
}
